<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo3.Registration_Controller">
   <children>
      <Label layoutX="20.0" layoutY="20.0" text="Registration Panel">
         <font>
            <Font size="40.0" />
         </font>
      </Label>
      <TextField fx:id="tfUserName" layoutX="20.0" layoutY="80.0" prefHeight="26.0" prefWidth="350.0" promptText="User Name" />
      <PasswordField fx:id="passField" layoutX="20.0" layoutY="125.0" prefHeight="26.0" prefWidth="350.0" promptText="Password" />
      <PasswordField fx:id="C_passField" layoutX="20.0" layoutY="170.0" prefHeight="26.0" prefWidth="350.0" promptText="Confirm Password" />
      <Button fx:id="btnSubmit" layoutX="156.0" layoutY="256.0" mnemonicParsing="false" onAction="#btnSubmitClicked" prefHeight="26.0" prefWidth="100.0" text="Submit" />
   </children>
</AnchorPane>
